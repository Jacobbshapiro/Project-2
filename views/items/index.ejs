<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Posted</th>
        <th>Title</th>
        <th>Price</th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach(function(i) { %>
        <tr>
          <td><%= i.title %></td>
          <td><%= i.releaseYear %></td>
          <td><%= i.posted %></td>
          <td><a href="/items/<%= i._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>